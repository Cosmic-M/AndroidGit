<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <solid
                android:color="#779f9d94"/>
            <stroke
                android:width="3dp"
                android:color="#171717" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#77343434"
                android:endColor="#77275296"
                android:angle="270"
                android:alpha="0.2"/>
            <stroke
                android:width="2dp"
                android:color="#171717" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="1dp"
                android:top="1dp"
                android:right="1dp"
                android:bottom="1dp" />
        </shape>
    </item>
</selector>


